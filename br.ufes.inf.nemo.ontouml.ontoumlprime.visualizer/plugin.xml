<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>

   <extension
         point="org.eclipse.ui.views">
      <category
            name="Main Category"
            id="br.ufes.inf.nemo.ontouml.ontoumlprime.visualizer">
      </category>
      <view
            name="OntoUMLPrime View"
            icon="icons/sample.gif"
            category="br.ufes.inf.nemo.ontouml.ontoumlprime.visualizer"
            class="br.ufes.inf.nemo.ontouml.ontoumlprime.visualizer.views.OntoUMLPrimeView"
            id="br.ufes.inf.nemo.ontouml.ontoumlprime.visualizer.views.OntoUMLPrimeView">
      </view>
   </extension>
   <extension
         point="org.eclipse.ui.perspectiveExtensions">
      <perspectiveExtension
            targetID="org.eclipse.jdt.ui.JavaPerspective">
         <view
               ratio="0.5"
               relative="org.eclipse.ui.views.ProblemView"
               relationship="right"
               id="br.ufes.inf.nemo.ontouml.ontoumlprime.visualizer.views.OntoUMLPrimeView">
         </view>
      </perspectiveExtension>
   </extension>
   <extension
         point="org.eclipse.help.contexts">
      <contexts
            file="contexts.xml">
      </contexts>
   </extension>
   
   <extension point="net.sourceforge.plantuml.eclipse.diagramTextProvider">
    <diagramTextProvider
      fileExtensions="ouml"
      providerClass="br.ufes.inf.nemo.ontouml.ontoumlprime.visualizer.plantuml.OntoUMLDiagramTextProvider">
    </diagramTextProvider>
  </extension> 

</plugin>
